# refer https://docs.conda.io/projects/conda/en/latest/configuration.html

allow_other_channels: true
override_channels_enabled: true
# had set the free channel url in default_channels, so set restore_free_channel to false
restore_free_channel: false

ssl_verify: false
safety_checks: disabled

#remote_connect_timeout_secs: 10.0
#remote_max_retries: 3
#remote_read_timeout_secs: 60.0

auto_update_conda: false
#channel_priority: strict
#channel_priority: disabled
channel_priority: flexible
notify_outdated_conda: true
report_errors: false
show_channel_urls: true
# verbosity: 0 is warn. 1 is info. 2 is debug. 3 is trace.
verbosity: 0


#proxy_servers:
#  http: socks5://127.0.0.1:1080
#  https: socks5://127.0.0.1:1080


channels:
  - defaults


default_channels:
  - https://mirrors.tuna.tsinghua.edu.cn/anaconda/pkgs/main/
  - https://mirrors.tuna.tsinghua.edu.cn/anaconda/pkgs/free/
  - https://mirrors.tuna.tsinghua.edu.cn/anaconda/pkgs/r/
  - https://mirrors.tuna.tsinghua.edu.cn/anaconda/cloud/msys2


